<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <ImageView
            android:layout_width="70dp"
            android:layout_height="70dp" app:srcCompat="@drawable/wheel"
            android:id="@+id/wheelOutsideImg" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            android:contentDescription="@string/app_name"/>
    <ImageView
            android:layout_width="50dp"
            android:layout_height="50dp" app:srcCompat="@drawable/wheel"
            android:id="@+id/wheelInsideImg"
            app:layout_constraintTop_toTopOf="@+id/wheelOutsideImg" app:layout_constraintBottom_toBottomOf="@+id/wheelOutsideImg"
            app:layout_constraintStart_toStartOf="@+id/wheelOutsideImg"
            app:layout_constraintEnd_toEndOf="@+id/wheelOutsideImg" android:layout_marginTop="16dp"
            android:layout_marginLeft="16dp" android:layout_marginStart="16dp" android:layout_marginRight="16dp"
            android:layout_marginEnd="16dp" android:layout_marginBottom="16dp"
            android:contentDescription="@string/app_name"/>
</androidx.constraintlayout.widget.ConstraintLayout>